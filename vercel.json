{
  "functions": {
    "api/index.ts": {
      "maxDuration": 25
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts",
      "headers": {
        "Cache-Control": "public, max-age=300, s-maxage=300"
      }
    },
    {
      "source": "/health",
      "destination": "/api/index.ts",
      "headers": {
        "Cache-Control": "public, max-age=60, s-maxage=60"
      }
    },
    {
      "source": "/",
      "destination": "/api/index.ts",
      "headers": {
        "Cache-Control": "public, max-age=3600, s-maxage=3600"
      }
    }
  ]
}
